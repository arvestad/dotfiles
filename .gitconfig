# This is Git's per-user configuration file.
[user]
# Please adapt and uncomment the following lines:
#	name = Lars Arvestad
#	email = arve@math.su.se
[user]
	name = arvestad
	email = arve@math.su.se

[color]
	branch = auto
	diff = auto
	status = auto
	interactive = auto
[color "branch"]
	current = yellow reverse
	local = yellow
	remote = green
[color "diff"]
	meta = yellow bold
	frag = magenta bold
	old = red bold
	new = green bold
[color "status"]
	added = yellow
	changed = green
	untracked = cyan

[core]
	editor = emacs -nw
	excludesFile = ~/.gitignore
[init]
	defaultBranch = main
